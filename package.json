{
  "name": "minimal-boilerplate",
  "version": "0.1.0",
  "description": "minimal-boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "serve": "http-server --path dist/",
    "clean": "rm -Rf dist/*",
    "watch": "NODE_ENV=development webpack --config webpack.dev.js --watch & npm run lint",
    "dev": "npm run watch & npm run serve",
    "lint": "eslint 'src/*.js' --fix & npx stylelint 'src/*.scss' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/da-silva-gomes/minimal-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "tailwind-css"
  ],
  "author": "Jos√© Gomes",
  "license": "MIT",
  "homepage": "https://github.com/da-silva-gomes/minimal-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@webpack-cli/init": "^1.1.2",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-google": "^0.14.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "scss-reset": "^1.1.10",
    "tailwindcss": "^2.0.2"
  }
}
